<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:id="@+id/activity_main"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical"
              android:paddingBottom="@dimen/activity_vertical_margin"
              android:paddingLeft="@dimen/activity_horizontal_margin"
              android:paddingRight="@dimen/activity_horizontal_margin"
              android:paddingTop="@dimen/activity_vertical_margin"
              tools:context="com.jibo.MainActivity">

    <Button
        android:id="@+id/btn_model_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="iot_Test"/>

    <Button
        android:id="@+id/btn_location_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="location_Test"/>

    <Button
        android:id="@+id/btn_usersave_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="test"/>

    <Button
        android:id="@+id/btn_loopssave_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="saveLoops"/>

    <Button
        android:id="@+id/btn_get"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get请求测试"/>

    <Button
        android:id="@+id/btn_post"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="post请求测试"/>

    <Button
        android:id="@+id/btn_pic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传图片"/>

    <Button
        android:id="@+id/btn_lication"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取地理位置"/>

    <Button
        android:id="@+id/btn_opengps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="打开GPS"/>

    <Button
        android:id="@+id/btn_checkpro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="检查权限"/>

    <Button
        android:id="@+id/btn_showpro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="提示权限"/>

    <Button
        android:id="@+id/btn_getroot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取root权限"/>

</LinearLayout>
